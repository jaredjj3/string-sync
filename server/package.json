{
  "name": "string-sync-server",
  "version": "1.0.0",
  "description": "Serve requests from stringsync.com.",
  "repository": "https://github.com/jaredjj3/string-sync",
  "author": "Jared Johnson",
  "license": "MIT",
  "private": true,
  "scripts": {
    "compile:db": "tsc --project ./tsconfig.db.json",
    "migrate": "sequelize db:migrate",
    "seed": "sequelize db:seed:all",
    "build": "NODE_ENV=production webpack",
    "build:dev": "webpack",
    "start": "node ./dst/index.js",
    "start:dev": "nodemon -V --watch ./dst/index.js",
    "start:scripts:dev": "yarn compile:db && yarn migrate && yarn seed && yarn compile:db --watch",
    "test": "jest"
  },
  "dependencies": {
    "apollo-server": "^2.9.3",
    "bcrypt": "^3.0.6",
    "cookie": "^0.4.0",
    "dataloader": "^1.4.0",
    "graphql": "^14.5.6",
    "lodash": "^4.17.15",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.19.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cookie": "^0.3.3",
    "@types/express": "^4.17.1",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^24.0.19",
    "@types/lodash": "^4.14.144",
    "@types/node": "^12.7.12",
    "@types/pg": "^7.11.1",
    "@types/sequelize": "^4.28.4",
    "jest": "^24.9.0",
    "nodemon": "^1.19.1",
    "sequelize-cli": "^5.5.1",
    "sequelize-mock": "^0.10.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.0.4",
    "typescript": "~3.4.3",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.6",
    "webpack-node-externals": "^1.7.2",
    "webpack-shell-plugin": "^0.5.0"
  }
}
