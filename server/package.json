    "sequelize": "^5.21.1",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cls-hooked": "^4.3.0",
    "@types/cookie": "^0.3.3",
    "@types/express": "^4.17.1",
