{
  "name": "string-sync-server",
  "version": "1.0.0",
  "description": "Serve requests from stringsync.com.",
  "repository": "https://github.com/jaredjj3/string-sync",
  "author": "Jared Johnson",
  "main": "server.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prepare": "mkdir -p build && touch ./build/server.js",
    "start": "yarn prepare && node ./build/server.js",
    "dev": "yarn prepare && nodemon --delay 500ms --watch build ./build/server.js",
    "watch": "tsc -w",
    "compile": "tsc -p .",
    "check": "gts check",
    "clean": "gts clean",
    "fix": "gts fix"
  },
  "dependencies": {
    "express": "4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "gts": "^1.0.0",
    "nodemon": "^1.19.1",
    "typescript": "~3.4.0"
  }
}
