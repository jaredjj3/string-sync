{
  "name": "string-sync-server",
  "version": "1.0.0",
  "description": "Serve requests from stringsync.com.",
  "repository": "https://github.com/jaredjj3/string-sync",
  "author": "Jared Johnson",
  "main": "server.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "ensure-server-file": "mkdir -p build && touch ./build/server.js",
    "start": "yarn ensure-server-file && node ./build/server.js",
    "dev": "yarn ensure-server-file && nodemon --delay 500ms ./build/server.js",
    "watch": "tsc -w",
    "compile": "tsc -p .",
    "check": "gts check",
    "clean": "gts clean",
    "fix": "gts fix"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^8.3.2",
    "bcryptjs": "^2.4.3",
    "graphql": "^14.4.2",
    "graphql-yoga": "^1.18.0",
    "jsonwebtoken": "^8.5.1",
    "prisma-client-lib": "1.34.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/graphql": "^14.2.2",
    "@types/node": "^12.0.12",
    "gts": "^1.0.0",
    "nodemon": "^1.19.1",
    "prisma": "^1.34.0",
    "rimraf": "^2.6.3",
    "ts-node": "^8.3.0",
    "typescript": "~3.4.0"
  }
}
