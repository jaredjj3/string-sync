{
  "name": "stringsync",
  "version": "1.0.0",
  "repository": "git@github.com:jaredjj3/string-sync.git",
  "private": true,
  "workspaces": [
    "modules/*"
  ],
  "scripts": {
    "graphql": "yarn workspace @stringsync/graphql",
    "db": "yarn workspace @stringsync/db",
    "web": "yarn workspace @stringsync/web",
    "dev": "yarn db migrate && yarn db seed && yarn graphql dev",
    "pretest:server": "yarn db migrate",
    "test:server": "jest",
    "test:web": "yarn web test"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@types/jest": "^24.0.19",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "babel-eslint": "10.x",
    "eslint": "^6.7.2",
    "eslint-config-react-app": "^5.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-flowtype": "3.13.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest-dom": "^1.3.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "^1.6.1",
    "eslint-plugin-testing-library": "^1.3.4",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "react": "^16.9.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.4"
  }
}
