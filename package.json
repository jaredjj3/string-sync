{
  "name": "string-sync-web-client-proxy",
  "scripts": {
    "start": "cd client && yarn run start",
    "storybook": "cd client && yarn run storybook",
    "build": "cd client && yarn install && yarn run build",
    "build-parser": "cd client/src/models/vextab/encoding && ../../../../node_modules/.bin/jison vextabDecoder.jison",
    "deploy": "cp -a client/build/. public/",
    "test": "cd client && yarn run test",
    "jest": "cd client && yarn run jest",
    "postinstall": "yarn run build && yarn run deploy && echo 'Client built!'"
  }
}
